{
  "manifest_version": 2,
  "name": "One click Github Commit",
  "description": "Silly idea from @HNOnion",
  "version": "1.0",
  "icons": {
    "32": "icon32.png",
    "200": "icon200.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["index.js"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "options_ui": {
      "page": "options.html"
  }
}
